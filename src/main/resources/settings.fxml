<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="589.0" prefWidth="590.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="filetransfer.controllers.SettingsController">
    <children>
        <Pane prefHeight="579.0" prefWidth="581.0">
            <children>
                <Button layoutX="34.0" layoutY="75.0" mnemonicParsing="false" onAction="#changeImagesPath"
                        prefHeight="81.0" prefWidth="522.0"
                        style="-fx-background-color: #FFFFFF; -fx-background-radius: 15;">
                    <graphic>
                        <Pane prefHeight="80.0" prefWidth="504.0">
                            <children>
                                <Label layoutX="14.0" layoutY="14.0" text="Images">
                                    <font>
                                        <Font size="17.0"/>
                                    </font>
                                </Label>
                                <Label fx:id="imagesPath" layoutX="14.0" layoutY="40.0"
                                       text="example images save path"/>
                            </children>
                        </Pane>
                    </graphic>
                </Button>
                <Button layoutX="34.0" layoutY="173.0" mnemonicParsing="false" onAction="#changeAudioPath"
                        prefHeight="81.0" prefWidth="522.0"
                        style="-fx-background-color: #FFFFFF; -fx-background-radius: 15;">
                    <graphic>
                        <Pane prefHeight="80.0" prefWidth="504.0">
                            <children>
                                <Label layoutX="14.0" layoutY="14.0" text="Audio">
                                    <font>
                                        <Font size="17.0"/>
                                    </font>
                                </Label>
                                <Label fx:id="audioPath" layoutX="14.0" layoutY="40.0" text="example images save path"/>
                            </children>
                        </Pane>
                    </graphic>
                </Button>
                <Button layoutX="34.0" layoutY="270.0" mnemonicParsing="false" onAction="#changeVideosPath"
                        prefHeight="81.0" prefWidth="522.0"
                        style="-fx-background-color: #FFFFFF; -fx-background-radius: 15;">
                    <graphic>
                        <Pane prefHeight="80.0" prefWidth="504.0">
                            <children>
                                <Label layoutX="14.0" layoutY="14.0" text="Videos">
                                    <font>
                                        <Font size="17.0"/>
                                    </font>
                                </Label>
                                <Label fx:id="videosPath" layoutX="14.0" layoutY="40.0"
                                       text="example images save path"/>
                            </children>
                        </Pane>
                    </graphic>
                </Button>
                <Button layoutX="34.0" layoutY="377.0" mnemonicParsing="false" onAction="#changeDocumentsPath"
                        prefHeight="81.0" prefWidth="522.0"
                        style="-fx-background-color: #FFFFFF; -fx-background-radius: 15;">
                    <graphic>
                        <Pane prefHeight="80.0" prefWidth="504.0">
                            <children>
                                <Label layoutX="14.0" layoutY="14.0" text="Documents">
                                    <font>
                                        <Font size="17.0"/>
                                    </font>
                                </Label>
                                <Label fx:id="documentsPath" layoutX="14.0" layoutY="40.0"
                                       text="example images save path"/>
                            </children>
                        </Pane>
                    </graphic>
                </Button>
                <Button layoutX="34.0" layoutY="475.0" mnemonicParsing="false" onAction="#changeOtherPath"
                        prefHeight="81.0" prefWidth="522.0"
                        style="-fx-background-color: #FFFFFF; -fx-background-radius: 15;">
                    <graphic>
                        <Pane prefHeight="80.0" prefWidth="504.0">
                            <children>
                                <Label layoutX="14.0" layoutY="14.0" text="Other">
                                    <font>
                                        <Font size="17.0"/>
                                    </font>
                                </Label>
                                <Label fx:id="otherPath" layoutX="14.0" layoutY="40.0" text="example images save path"/>
                            </children>
                        </Pane>
                    </graphic>
                </Button>
                <Label layoutX="226.0" layoutY="40.0" text="Default Save Paths">
                    <font>
                        <Font name="Verdana Bold" size="13.0"/>
                    </font>
                </Label>
            </children>
        </Pane>
    </children>
</VBox>
